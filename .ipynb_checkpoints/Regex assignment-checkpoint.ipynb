    "\n",
    "handle = open('regex_sum_527439.txt')\n",
    "nums = []\n",
    "for line in handle:\n",
    "    extract = re.findall('[0-9]+', line)\n",
    "    nums += extract\n",
    "nums = [int(i) for i in nums]\n",
    "total = sum(nums)\n",
    "total"
